{
  "name": "ahmedwassef/laravel-email-sentry",
  "description": "Library for email management and tracking in Laravel apps.",
  "keywords": [
    "laravel",
    "php",
    "email sentry",
    "Eamil Auditing"
  ],
  "type": "package",
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmed Wassef",
      "email": "ahmedwassef2015@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "laravel/framework": "^8.37|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Ahmedwassef\\LaravelEmailSentry\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ahmedwassef\\LaravelEmailSentry\\LaravelEmailSentryServiceProvider"
      ],
      "aliases": {
        "MailSentry": "Ahmedwassef\\LaravelEmailSentry\\Facades\\EmailSentry"
      }
    }
  }
}
